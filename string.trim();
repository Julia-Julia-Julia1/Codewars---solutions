// Training JS #21: Methods of String object--trim() and the string template

let str="             abc                ";
console.log(str.trim());
str="\n\n\n\nabc\t\t\t"
console.log(str.trim());


function fiveLine(s){
 let str = s.trim();
  let result = "";
  for (let i = 1; i < 6; i++) {
  result += str.repeat(i) + "\n"
  } 
  return result.trim() 
}



